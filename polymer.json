{
  "lint": {
    "rules": [
      "polymer-3"
    ]
  },
  "entrypoint": "demo/index.html",
  "extraDependencies": [
  	"sample-lit-element.js"
  ],
  "builds": [
  	{
  		"name": "es6-bundled",
  		"browserCapabilities": [
  		  "es2015"
  		],
  		"js": {
  		  "compile": "es2015",
  		  "minify": true,
  		  "transformModulesToAmd": true
  		},
  		"css": {
  		  "minify": true
  		},
  		"html": {
  		  "minify": true
  		},
  		"bundle": true,
  		"addServiceWorker": false
  	}
  ],
  "moduleResolution": "node",
  "npm": true
}
